{
  "name": "sharp-dance",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://elreyb.github.io/sharp-dance",
  "dependencies": {
    "env-cmd": "^10.0.1",
    "firebase": "^7.2.0",
    "firebase-admin": "^8.1.0",
    "focus-trap-react": "10.0.1",
    "formik": "^2.1.4",
    "lodash": "^4.17.11",
    "marked": "^4.0.10",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-firestore": "^1.0.1",
    "react-icons": "^3.7.0",
    "react-images": "^1.2.0-beta.7",
    "react-player": "^2.10.1",
    "react-router": "^6.4.3",
    "react-router-dom": "^6.4.3",
    "react-scripts": "5.0.1",
    "react-slick": "^0.29.0",
    "styled-components": "^5.1.0",
    "textfit": "2.4.0",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "yarn && react-scripts start",
    "build": "rm -rf build && react-scripts build",
    "lint": "prettier --check \"src/**/*.{js,json}\" --write && eslint src --fix",
    "test": "react-scripts test",
    "deploy": "gh-pages -d build --message \"[skip ci] Updates\" --repo \"https://$GH_TOKEN@github.com/ElreyB/sharp-dance.git\"",
    "predeploy": "yarn build",
    "optimize": "./optimize.sh",
    "resolve:security": "npm i --package-lock-only && npm audit fix && rm yarn.lock && yarn import && rm package-lock.json",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "babelMacros": {
    "styledComponents": {}
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-plugin-macros": "^3.1.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.27.0",
    "eslint-plugin-react": "^7.13.0",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.0",
    "pages": "^0.0.16",
    "prettier": "^2.0.4"
  }
}
